{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "cli": {
        "warnings": {
            "typescriptMismatch": false
        }
    },
    "projects": {
        "metaheuristic-app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "sass"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/metaheuristic-app",
                        "resourcesOutputPath": "resources",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "es5BrowserSupport": true
                    },
                    "configurations": {
                        "ric411": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.ric411.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "20mb",
                                "maximumError": "50mb"
                            }]
                        },
                        "ric411-prod": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.ric411-prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "20mb",
                                "maximumError": "50mb"
                            }]
                        },
                        "ric355": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.ric355.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "20mb",
                                "maximumError": "50mb"
                            }]
                        },
                        "ric364": {
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.ric364.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [{
                                "type": "initial",
                                "maximumWarning": "20mb",
                                "maximumError": "50mb"
                            }]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "metaheuristic-app:build"
                    },
                    "configurations": {
                        "ric411": {
                            "browserTarget": "metaheuristic-app:build:ric411"
                        },
                        "ric411-prod": {
                            "browserTarget": "metaheuristic-app:build:ric411-prod"
                        },
                        "ric355": {
                            "browserTarget": "metaheuristic-app:build:ric355"
                        },
                        "ric364": {
                            "browserTarget": "metaheuristic-app:build:ric364"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "metaheuristic-app:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "metaheuristic-app-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "metaheuristic-app:serve"
                    },
                    "configurations": {
                        "ric411": {
                            "devServerTarget": "metaheuristic-app:serve:ric411"
                        },
                        "ric411-prod": {
                            "devServerTarget": "metaheuristic-app:serve:ric411-prod"
                        },
                        "ric355": {
                            "devServerTarget": "metaheuristic-app:serve:ric355"
                        },
                        "ric364": {
                            "devServerTarget": "metaheuristic-app:serve:ric364"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "metaheuristic-app"
}