{
  "name": "metaheuristic-app",
  "version": "0.0.1",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build",
    "build2": "ng build --baseHref=/aiai-app/ --prod=true",
    "build3": "ng build --prod=true",
    "build4": "ng build --aot --prod=true",
    "build5": "ng build --aot",
    "npm-install": "npm i",
    "npm-install2": "npm install -g npm",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "review": "ng build --stats-json && webpack-bundle-analyzer dist/metaheuristic-app/stats-es2015.json",
    "review-prod": "ng build --prod=true --stats-json && webpack-bundle-analyzer dist/metaheuristic-app/stats-es2015.json",
    "extract-i18n": "ngx-translate-extract  -p /**/*.html /**/*.ts /**/*.pug --input ./src --output ./src/assets/i18n/EN.json ./src/assets/i18n/RU.json --clean --sort --format json --marker marker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.0",
    "@angular/cdk": "9.2.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@ngrx/effects": "8.5.2",
    "@ngrx/store": "8.5.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "angular-plotly.js": "1.3.2",
    "angular2-notifications": "^3.0.0",
    "core-js": "2.6.9",
    "file-saver": "2.0.2",
    "hammerjs": "2.0.8",
    "ngx-json-viewer": "2.4.0",
    "node-sass": "4.12.0",
    "normalize.css": "8.0.1",
    "plotly.js": "1.49.4",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@biesbjerg/ngx-translate-extract": "3.0.5",
    "@types/jasmine": "2.8.16",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "8.10.53",
    "apply-loader": "2.0.0",
    "codelyzer": "5.0.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.2",
    "ts-node": "7.0.1",
    "tslint": "5.20.0",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.4.1"
  }
}
